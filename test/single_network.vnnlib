(declare-network acc 
	(declare-input X Real [3])
	(declare-output Y Real [4])
)

(assert (<= (* -1.0 X[0]) 0.0))
(assert (<= X[0] 50.0))
(assert (<= (* -1.0 X[1]) 50.0))
(assert (<= X[1] 50.0))
(assert (<= (* -1.0 X[2]) 0.0))
(assert (<= X[2] 150.0))
(assert (<= (- (* 1.5 X[1]) X[2]) -5.0))

(assert (<= (* -1.0 Y[0]) 0.0))
(assert (<= Y[0] 1.0))

; weak left is minimal or COC is minimal
(assert (and
    (or (<= Y[2] Y[0]) (<= Y[2] Y[1]))
    (or (<= Y[3] Y[0]) (<= Y[3] Y[1]))
    (or (<= Y[3] Y[0]) (<= Y[3] Y[1]))
))